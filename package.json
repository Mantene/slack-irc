{
  "name": "slack-irc",
  "version": "3.8.1",
  "description": "Connects IRC and Slack channels by sending messages back and forth.",
  "keywords": [
    "slack",
    "irc",
    "gateway",
    "bot",
    "slack-irc"
  ],
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ekmartin/slack-irc.git"
  },
  "engines": {
    "node": ">=4"
  },
  "bugs": {
    "url": "https://github.com/ekmartin/slack-irc/issues"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "babel lib --out-dir dist",
    "prepublish": "npm run build",
    "lint": "eslint . --ignore-path .gitignore",
    "mocha": "mocha --compilers js:babel-core/register $(find test -name '*.test.js')",
    "mocha:watch": "npm run mocha -- --watch --reporter min",
    "coverage": "nyc --require babel-core/register _mocha -- $(find test -name '*.test.js') && nyc report --reporter=cobertura",
    "test": "npm run lint && npm run coverage"
  },
  "author": {
    "name": "Martin Ek <mail@ekmartin.no>"
  },
  "license": "MIT",
  "dependencies": {
    "@slack/client": "^3.15.0",
    "check-env": "1.2.0",
    "commander": "2.9.0",
    "gists": "^1.0.1",
    "html-entities": "^1.2.1",
    "imgur": "^0.2.1",
    "irc": "0.5.0",
    "language-classifier": "0.0.1",
    "lodash": "^4.17.4",
    "request": "^2.76.0",
    "strip-json-comments": "2.0.1",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.16.0",
    "mocha": "^3.5.3",
    "nyc": "^8.4.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.14.0"
  }
}
